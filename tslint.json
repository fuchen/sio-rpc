{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "semicolon": [true, "never"],
        "no-empty": false,
        "trailing-comma": false,
    },
    "rulesDirectory": []
}
